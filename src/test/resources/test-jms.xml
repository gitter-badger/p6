<?xml version="1.0" encoding="UTF-8"?>
<messaging-deployment xmlns="urn:jboss:messaging-activemq-deployment:1.0">
  <server name="default">
    <jms-destinations>

      <jms-queue name="p6.activity">
        <entry name="queue/p6.activity" />
        <entry name="java:/jms/queue/p6.activity" />
      </jms-queue>

      <jms-queue name="p6.execution">
        <entry name="queue/p6.execution" />
        <entry name="java:/jms/queue/p6.execution" />
      </jms-queue>

      <jms-queue name="p6.singleton">
        <entry name="queue/p6.singleton" />
        <entry name="java:/jms/queue/p6.singleton" />
      </jms-queue>
      
    </jms-destinations>
  </server>
</messaging-deployment>